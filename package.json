{
  "name": "vue-router",
  "version": "0.1.0",
  "description": "A router for Vue.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "node example/serve.js & webpack --watch --config build/webpack.example.config.js",
    "min": "uglifyjs dist/vue-router.js -c -m > dist/vue-router.min.js",
    "build": "webpack --config build/webpack.build.config.js && npm run min",
    "test": "webpack --config build/webpack.test.config.js && ./node_modules/karma/bin/karma start build/karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-router.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "router",
    "mvvm"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vue-router/issues"
  },
  "homepage": "https://github.com/vuejs/vue-router#readme",
  "devDependencies": {
    "express": "^4.12.3",
    "jasmine-core": "^2.3.2",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "uglify-js": "^2.4.23",
    "vue": "yyx990803/vue#dev",
    "webpack": "^1.8.11"
  },
  "dependencies": {
    "route-recognizer": "^0.1.5",
    "insert-css": "^0.2.0"
  }
}
