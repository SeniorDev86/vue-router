{
  "name": "vue-router",
  "version": "0.1.0",
  "description": "A router for Vue.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "npm run serve & npm run watch-test",
    "lint": "eslint src && eslint build",
    "test": "npm run lint && ./node_modules/karma/bin/karma start build/karma.config.js",
    "build": "webpack --config build/webpack.build.dev.config.js && webpack --config build/webpack.build.min.config.js",
    "serve": "webpack-dev-server --host 0.0.0.0 --quiet --config example/webpack.config.js --content-base example --history-api-fallback",
    "watch-test": "webpack --watch test/test.js test/test.build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-router.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "router",
    "mvvm"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vue-router/issues"
  },
  "homepage": "https://github.com/vuejs/vue-router#readme",
  "devDependencies": {
    "css-loader": "^0.15.4",
    "eslint": "^0.24.0",
    "express": "^4.12.3",
    "html-loader": "^0.3.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.2",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-sauce-launcher": "^0.2.10",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.6.0",
    "style-loader": "^0.12.3",
    "vue": "yyx990803/vue#dev",
    "vue-loader": "^2.1.1",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "route-recognizer": "^0.1.5"
  }
}
