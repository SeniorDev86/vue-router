{
  "name": "vue-router",
  "version": "0.1.0",
  "description": "A router for Vue.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "node example/serve.js & webpack --watch --config build/webpack.dev.config.js & webpack --watch --config build/webpack.test.config.js",
    "lint": "eslint src && eslint build",
    "build": "webpack --config build/webpack.build.config.js && webpack --config build/webpack.min.config.js",
    "build-test": "webpack --config build/webpack.test.config.js",
    "test": "npm run lint && ./node_modules/karma/bin/karma start build/karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-router.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "router",
    "mvvm"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vue-router/issues"
  },
  "homepage": "https://github.com/vuejs/vue-router#readme",
  "devDependencies": {
    "css-loader": "^0.15.4",
    "eslint": "^0.24.0",
    "express": "^4.12.3",
    "html-loader": "^0.3.0",
    "jasmine-core": "^2.3.2",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-commonjs": "^0.0.10",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.10",
    "karma-webpack": "^1.6.0",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.23",
    "vue": "yyx990803/vue#dev",
    "vue-loader": "^2.1.1",
    "webpack": "^1.10.5"
  },
  "dependencies": {
    "route-recognizer": "^0.1.5"
  }
}
